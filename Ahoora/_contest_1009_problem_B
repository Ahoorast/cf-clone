#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	string s, t;
	cin >> s;
	int n = s.size(), cnt = 0;
	for (int i = 0; i < n; i++)
		if (s[i] == '1')
			cnt++;
		else
			t += s[i];
	int i = 0;
	while (i < t.size() && t[i] < '1')
		cout << t[i], i++;
	for (int i = 1; i <= cnt; i++)
		cout << 1;
	for (; i < t.size(); i++)
		cout << t[i];
	cout << endl;
	return 0;
}
