#include <bits/stdc++.h>
using namespace std;
Â 
int main()
{
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);	
	int n;
	string s;
	cin >> n;
	while (n--) {
		cin >> s;
		sort(s.begin(), s.end());
		int sz = s.size(), res = 0;
		for (int i = 0; i + 1 < sz; i++)
			if(s[i] + 1 != s[i + 1]) res++;
		cout << (res? "NO": "YES") << endl;
	}
	return 0;
}
