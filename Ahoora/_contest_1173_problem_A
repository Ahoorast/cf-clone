#include <bits/stdc++.h>
using namespace std;
int main()
{
	int a, b, c, sum = 0;
	cin >> a >> b >> c;
	sum = a - b; 
	if(sum - c < 0 && sum + c >= 0)
	cout << "?";
	else if(sum - c <= 0 && sum + c > 0)
	cout << "?";
	else if(sum > 0)
	cout << "+";
	else if(sum < 0) 
	cout << "-";
	else 
	cout << "0";
	return 0;
}
