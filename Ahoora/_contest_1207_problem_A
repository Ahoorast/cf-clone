#include <bits/stdc++.h>
using namespace std;
Â 
int main()
{
	int t, p, c, p1, p2, b;
	cin >> t;
	for(int i = 0; i < t; i++) {
		cin >> b >> p >> c >> p1 >> p2;
		int bo = 0;
		if(p1 < p2)
			bo++;
		b /= 2;
		long long ans = 0;
		if(bo == 0) {
			ans += min(b, p) * p1;
			int x;
			x = min(b, p);
			p -= x;
			b -= x;
			if(b > 0) {
				ans += min(b, c) * p2;
			}
		}
		else if(bo == 1) {
			ans += min(b, c) * p2;
			int x;
			x = min(b, c);
			c -= x;
			b -= x;
			if(b > 0) 
				ans += min(b, p) * p1;
		}
		cout << ans << endl;
	}
	return 0;
}
