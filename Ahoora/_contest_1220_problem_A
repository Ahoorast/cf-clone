#include <bits/stdc++.h>
using namespace std;
Â 
int main()
{
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	int n, cnt = 0;
	string s;
	cin >> n >> s;
	for (int i = 0; i < n; i++) 
		if(s[i] == 'n') cnt++;
	for (int i = 0; i < cnt; i++) cout << "1 ";
	for (int i = 0; i < (n - (cnt * 3)) / 4; i++) cout << "0 ";	 	
	return 0;
}
