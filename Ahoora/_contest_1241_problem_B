#include <bits/stdc++.h>
using namespace std;
 
int arr[30];
 
int main() {
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	long long q;
	string s, t;
	cin >> q;
	while (q--) {
		cin >> s >> t;
		int sz = s.size(), tz = t.size(), res = 0;
		for (int i = 0; i < sz; i++)
			for (int j = 0; j < tz; j++)
				if (s[i] == t[j])
					res = 1;
		cout << (res == 1? "YES": "NO") << endl;
	}
	return 0;
}
