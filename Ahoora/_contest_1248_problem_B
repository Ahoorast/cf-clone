#include <bits/stdc++.h>
using namespace std;
 
const int N = 100 * 1000 + 100;
int a[N];
Â 
int main() {
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	unsigned long long res = 0, s1 = 0, s2 = 0;
	int n;
	cin >> n;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	sort(a, a + n);
	for (int i = 0; i < n / 2; i++)
		s1 += a[i];
	for (int i = 0; i < n; i++)
		s2 += a[i];
	s2 -= s1;
	res = s2 * s2 + s1 * s1;
	cout << res << endl;
	return 0;
}
