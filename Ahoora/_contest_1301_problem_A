#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	int Q;
	cin >> Q;
	while (Q--) {
		string a, b, c;
		cin >> a >> b >> c;
		int n = a.size();
		bool res = true;
		for (int i = 0; i < n; i++)
			res &= (c[i] == a[i] || c[i] == b[i]);
		cout << (res? "YES": "NO") << endl;
	}
	return 0;
}
