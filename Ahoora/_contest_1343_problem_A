#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long n, res;
		cin >> n;
		for (int i = 1; i <= 31; i++) {
			long long x = (1LL << i) - 1;
			if (n % x == 0)
				res = n / x;
		}
		cout << res << endl;
	}
	return 0;
}
