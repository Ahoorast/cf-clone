#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		string s;
		cin >> s;
		int ans = 0, sz = s.size();
		for (int i = 0; i < sz; i++)
			ans += s[i] != '0';
		cout << ans << endl;	
		for (int i = 0; i < sz; i++) 
			if (s[i] != '0') {
				cout << s[i];
				for (int j = sz - 1; j > i; j--)
					cout << '0';
				cout << ' ';
			}
		cout << endl;
	}
	return 0;
}
