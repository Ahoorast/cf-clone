#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		unsigned long long n, k;
		cin >> n >> k;
		k--;
		while (k--) {
			int mn = 10, mx = 0;
			unsigned long long t = n;
			while (t)
				mn = min(1uLL * mn, t % 10), mx = max(1uLL * mx, t % 10), t /= 10;
			if (mn == 0)
				break;
			n += mn * mx;
		}
		cout << n << endl;
	}
	return 0;
}
