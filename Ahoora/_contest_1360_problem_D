#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	int Q;
	cin >> Q;
	while (Q--) {
		int n, k, ans;
		cin >> n >> k;
		ans = n;
		for (int i = 2; 1LL * i * i <= n; i++)
			if (n % i == 0) {
				if (i <= k)
					ans = min(ans, n / i);
				if (n / i <= k)
					ans = min(ans, i);
			}
		if (k >= n)
			ans = 1;
		cout << ans << '\n';
	}
	return 0;
}
