#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long a, b;
		cin >> a >> b;
		if (a < b)
			swap(a, b);
		int ans = 0;
		while (a > b) {
			ans++;
			b <<= 1;
		}
		if (a == b) {
			int res = 1 << 30;
			for (int i = 0; i * 3 <= ans; i++)
				for (int j = 0; j * 2 <= ans - i * 3; j++)
					res = min(res, i + j + ans - i * 3 - j * 2);
			cout << res << '\n';
		}
		else
			cout << - 1 << '\n';
	}
	return 0;
}
