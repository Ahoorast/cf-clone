#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		string b;
		cin >> b;
		cout << b[0] << b[1];
		int n = b.size();
		for (int i = 3; i < n; i += 2)
			cout << b[i];
		cout << '\n';
	}
	return 0;
}
