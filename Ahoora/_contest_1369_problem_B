#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		string s, ans;
		cin >> n >> s;
		int l = 0, r = n - 1;
		while (s[l] == '0')
			l++;
		l--;
		while (s[r] == '1')
			r--;
		r++;
		for (int i = 0; i <= l; i++)
			ans += '0';
		l = max(l, 0), r = min(r, n);
		l++;
		if (l < r)
			ans += '0';
		for (int i = 0; i < n - r; i++)
			ans += '1';
		cout << ans << '\n';
	}
	return 0;
}
