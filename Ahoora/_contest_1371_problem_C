#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long a, b, n, m;
		cin >> a >> b >> n >> m;
		if (a < b)
			swap(a, b);
		if (m + n > a + b)
			cout << "No\n";
		else if (min(a, b) < m)
			cout << "No\n";
		else 
			cout << "Yes\n";
	}
	return 0;
}
