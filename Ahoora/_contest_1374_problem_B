#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		int cnt[2] = {0, 0};
		while (n % 3 == 0)
			cnt[0]++, n /= 3;
		while (n % 2 == 0)
			cnt[1]++, n /= 2;
		int mn = min(cnt[0], cnt[1]);
		cnt[0] -= mn, cnt[1] -= mn;
		if (n > 1 || (cnt[1] > cnt[0]))
			cout << "-1\n";
		else 
			cout << mn + max(cnt[0], cnt[1]) * 2 << '\n';
	}
	return 0;
}
