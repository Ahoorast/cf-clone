#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		int x = n / 4 + (n % 4 != 0);
		for (int i = 0; i < n - x; i++)
			cout << '9';
		for (int i = 0; i < x; i++)
			cout << '8';
		cout << '\n';
	}
	return 0;
}
