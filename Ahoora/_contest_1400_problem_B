#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long p, f, cnts, cntw, s, w;
		cin >> p >> f >> cnts >> cntw >> s >> w;
		if (s > w)
			swap(s, w), swap(cnts, cntw);
		if (p < f)
			swap(p, f);
		int ans = 0;
		for (int i = 0; i <= cnts; i++) {
			if (s * i > p)
				continue;
			int tans = i;
			long long cntww = cntw;
			long long cntss = cnts;
			long long pp = p - s * i;
			long long ff = f;
			long long t = min(cnts - i, ff / s);
			ff -= s * t, tans += t;
			t = min(cntww, pp / w);
			tans += t, pp -= w * t, cntww -= t;
			t = min(cntww, ff / w);
			tans += t;
			ans = max(ans, tans);
		}
		cout << ans << '\n';
	}
	return 0;
}
