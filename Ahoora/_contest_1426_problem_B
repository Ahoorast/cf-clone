#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n, a[2][2], m;
		bool res = false;
		cin >> n >> m;
		while (n--) {
			for (int i = 0; i < 2; i++)
				for (int j = 0; j < 2; j++)
					cin >> a[i][j];
			res |= a[0][1] == a[1][0];
		}
		cout << (res && m % 2 == 0? "YES": "NO") << '\n';
	}
	return 0;
}
