#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		bool fnd = false;
		for (int i = 0; 3 * i <= n; i++)
			for (int j = 0; j * 5 <= n; j++) {
				int x = 3 * i + 5 * j;
				if ((n - x) % 7 == 0 && !fnd) {
					cout << i << ' ' << j << ' ' << (n - x) / 7 << '\n';
					fnd = true;
					break;
				}
			}
		if (!fnd)
			cout << "-1\n";
	}
	return 0;
}
