#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long n, m, a, sum = 0;
		cin >> n >> m;
		for (int i = 1; i <= n; i++)
			cin >> a, sum += a;
		if (sum == m)
			cout << "YES\n";
		else
			cout << "NO\n";
	}
	return 0;
}
