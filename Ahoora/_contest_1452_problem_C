#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		string s;
		cin >> s;
		int n = (int)(s.size());
		int ans = 0;
		int s1 = 0, s2 = 0;
		for (int i = 0; i < n; i++) {
			s1 += (s[i] == '(');
			s2 += (s[i] == '[');
			if (s[i] == ']' && s2) {
				s2--;
				ans++;
			}
			if (s[i] == ')' && s1) {
				s1--;
				ans++;
			}
		}
		cout << ans << '\n';
	}
	return 0;
}
