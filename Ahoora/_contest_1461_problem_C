#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	cout << setprecision(6) << fixed;
	int Q;
	cin >> Q;
	while (Q--) {
		int n, m;
		cin >> n >> m;
		vector<int> a(n), p(n);
		vector<double> pp(n);
		fill(pp.begin(), pp.end(), 1.);
		for (int i = 0; i < n; i++) {
			cin >> a[i];
			p[--a[i]] = i;
		}
		for (int i = 0; i < m; i++) {
			int x;
			double y;
			cin >> x >> y;
			--x;
			pp[x] *= (1. - y);
		}
		for (int i = n - 2; i >= 0; i--)
			pp[i] *= pp[i + 1];
		bool fnd = false;
		for (int i = n - 1; i >= 0; i--)
			if (a[i] != i && !fnd) {
				cout << 1. - pp[i] << '\n';
				fnd = true;
			}
		if (!fnd)
			cout << 1. << '\n';
	}
	return 0;
}
