#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long a, b, c;
		cin >> a >> b >> c;
		long long s = a + b + c;
		long long ted = s / 9;
		if (min({a, b, c}) < ted)
			cout << "NO\n";
		else
			cout << (s % 9 == 0? "YES": "NO") << '\n';
	}
	return 0;
}
