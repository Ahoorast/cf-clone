#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		for (int i = 0, a; i < n; i++) {
			cin >> a;
			while ((a & (-a)) ^ a)
				a ^= a & (-a);
			cout << a << ' ';
		}
		cout << '\n';
	}
	return 0;
}
