#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		vector<int> x(n);
		for (auto &a: x)
			cin >> a;
		set<int> s;
		for (int i = 0; i < n; i++)
			for (int j = i + 1; j < n; j++)
				s.insert(abs(x[j] - x[i]));
		cout << (int)(s.size()) << '\n';
	}
	return 0;
}
