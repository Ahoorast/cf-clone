#include<bits/stdc++.h>
using namespace std;
 
string s = "989";
 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		for (int i = 1; i <= min(3, n); i++)
			cout << s[i - 1];
		for (int i = 0; i < n - 3; i++)
			cout << i % 10;
		cout << '\n';
	}
	return 0;
}
