#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	int num = 0;
	set<pair<int, int>> s, S;
	while (Q--) {
		int t;
		cin >> t;
		if (t == 1) {
			num++;
			int m;
			cin >> m;
			s.insert({-m, num});
			S.insert({num, -m});
		} else if (t == 2) {
			auto it = *S.begin();
			cout << it.first << '\n';
			S.erase(it);
			swap(it.first, it.second);
			s.erase(it);
		} else if (t == 3) {
			auto it = *s.begin();
			cout << it.second << '\n';
			s.erase(it);
			swap(it.first, it.second);
			S.erase(it);
		}
	}
	return 0;
}
