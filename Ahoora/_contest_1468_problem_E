void solve() {
	vector <int> a(4);
	for (int i = 0; i < 4; i++)
		cin >> a[i];
	
	sort(a.begin(), a.end());
	
	cout << a[0] * a[2] << endl;	
}
 
int main() {
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	int q = 1;
	cin >> q;
	
	while (q--) {
		solve();
	}
}
