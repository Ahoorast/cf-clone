#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	int t;
	string s;
	cin >> t;
	while (t--) {
		cin >> s;
		vector<pair<int, int>> vec;
		pair<int, int> ans;
		int x = 0, y = 0, tx, ty;
		for (char i : s) {
			if(i == 'L')
				x--;
			if (i == 'R')
				x++;
			if (i == 'U')
				y++;
			if (i == 'D')
				y--;
			vec.push_back({x, y});
		}
		bool c = 0;
		if (x == 0 && y == 0) {
			cout <<"1000000000 1000000000 \n";
			continue;
		}
		for (int i = 0; i < vec.size(); i++) {
			int bx = vec[i].first, by = vec[i].second;
			x = y = 0;
			for (char i : s) {
				tx = x, ty = y;
				if (i == 'L')
					x--;
				if (i == 'R')
					x++;
				if (i == 'U')
					y++;
				if (i == 'D')
					y--;
				if (x == bx && y == by)
					x = tx, y = ty;
			}
			if (x == 0 && y == 0)
				c = 1, ans = {bx, by};
		}
		if (c)
			cout << ans.first << " " << ans.second << endl;
		else
			cout << "0 0\n";
	}
}
