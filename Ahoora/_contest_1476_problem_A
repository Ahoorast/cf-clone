#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long n, k;
		cin >> n >> k;
		k = ((n + k - 1) / k) * k;
		long long ans = k / n;
		ans += (k % n != 0);
		cout << ans << '\n';
	}
	return 0;
}
