#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	string s;
	cin >> s;
	vector<int> vec;
	for (auto c: s)
		vec.push_back(c - 'A');
	int sz = (int)(vec.size());
	for (int i = 2; i < sz; i++)
		if ((vec[i - 1] + vec[i - 2]) % 26 != vec[i])
			return cout << "NO\n", 0;
	cout << "YES\n";
	return 0;
}
