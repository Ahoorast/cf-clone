#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		string s, t;
		cin >> s >> t;
		reverse(s.begin(),s.end());
		reverse(t.begin(),t.end());
		int n = (int)(s.size()), m = (int)(t.size()), j = 0;
		for (int i = 0; i < n; i++) {
			if (j == m)
				break;
			if (s[i] == t[j])
				j++;
			else
				i++;
		}
		cout << (j == m? "YES": "NO") << '\n';
	}
	return 0;
}
