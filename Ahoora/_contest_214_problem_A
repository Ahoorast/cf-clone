#include <bits/stdc++.h>
using namespace std;
 
const int N = 1000;
 
int main() {
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0); 
	int n, m, res = 0;
	cin >> n >> m;
	for (int i = 0; i < N; i++)
		for (int j = 0; j < N; j++)
			if (i * i + j == n && i + j * j == m)
				res++;
	cout << res << endl;	
	return 0;
}
