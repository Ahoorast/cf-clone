#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int a[5][5], s[5] = {0};
	for (int i = 1; i <= 3; i++)
		for (int j = 1; j <= 3; j++)
			cin >> a[i][j], s[i] += a[i][j];
	int mx = *max_element(s + 1, s + 4) + 1, sum = 0;
	for (int i = 1; i <= 3; i++)
		sum += (a[i][i] = mx - s[i]);
	for (int i = 1; i <= 3; i++, cout << endl)
		for (int j = 1; j <= 3; j++)
			if (i == j)
				cout << a[i][j] + max((mx + 1 - sum), 0) / 2 << ' ';
			else
				cout << a[i][j] << ' ';
	return 0;
}
