#include <bits/stdc++.h>
using namespace std;
Â 
int main()
{
	int r, c, ans = 0;
	string s[20];
	cin >> r >> c;
	for(int i = 0; i < r; i++) cin >> s[i];
	for(int i = 0; i < r; i++) {
		int cnt = 0;
		for(int j = 0; j < c; j++) {
			if(s[i][j] == 'S') cnt++;
		}
		if(cnt == 0) {
			for(int j = 0; j < c; j++) {
				if(s[i][j] != '-') {
					ans++;
					s[i][j] = '-';
				}
			}
		}
	}
	for(int i = 0; i < c; i++) {
		int cnt = 0;
		for(int j = 0; j < r; j++) {
			if(s[j][i] == 'S') cnt++;
		}
		if(cnt == 0) {
			for(int j = 0; j < r; j++) {
				if(s[j][i] != '-') {
					ans++;
					s[j][i] = '-';
				}
			}
		}
	}
	cout << ans;	
	return 0;
}
