#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int n, cnt1 = 0, cnt2 = 0;
	cin >> n;
	for (int i = 0, a; i < n; i++)
		cin >> a, (a == 100? cnt1++: cnt2++);
	for (int i = 0; i <= cnt1; i++)
		for (int j = 0; j <= cnt2; j++)
			if (i + 2 * j == (cnt1 - i) + 2 * (cnt2 - j))
				return cout << "YES" << endl, 0;
	cout << "NO" << endl;
	return 0;
}
