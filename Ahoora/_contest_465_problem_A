#include <iostream>
using namespace std;
int main()
{
	int n, arr[199], ans = 0, cnt = 0, a1;
	cin >> n;
	string a;
	cin >> a;
	for(int i = 0; i < n; i++)
	{
		arr[i] = a[i] - '0';
	}
	for(int i = 0; i < n; i++)
	{
		if(arr[i] == 1)
		{
			cnt++;
		}
		else
		{
			cout << cnt + 1;
			return 0;
		}
	}
	cout << cnt;
	return 0;
}
