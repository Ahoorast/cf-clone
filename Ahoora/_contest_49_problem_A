#include <iostream>
using namespace std;
int main()
{
    string a;
    getline (cin, a);
    int az = a.size() - 1;
    while(true)
    {
    	if(a[az] == ' ' || a[az] == '?')
    	az--;
    	else
    	break;
	} 
    if(a[az] == 'A' || a[az] == 'E' || a[az] == 'I' || a[az] == 'O' || a[az] == 'U' || a[az] == 'Y' || a[az] == 'a' || a[az] == 'e' || a[az] == 'i' || a[az] == 'o' || a[az] == 'u' || a[az] == 'y')
    cout << "YES";
    else
    cout << "NO";
    return 0;
}
