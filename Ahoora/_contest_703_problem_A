#include <bits/stdc++.h>
using namespace std;
int main()
{
	int n, ans = 0, a, b;
	cin >> n;
	for(int i = 0; i < n; i++)
	{
		cin >> a >> b;
		if(a > b)
		ans++;
		else if(b > a)
		ans--;
	}
	if(ans > 0)
	cout << "Mishka";
	else if (ans < 0)
	cout << "Chris";
	else 
	cout << "Friendship is magic!^^";
	return 0;
}
