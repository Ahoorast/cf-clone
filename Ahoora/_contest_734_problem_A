#include <bits/stdc++.h>
using namespace std;
int main()
{
	int as = 0, sz, n;
	string s;
	cin >> n >> s;
	sz = s.size();
	for(int i = 0; i < sz; i++)
	{
		if(s[i] == 'A')
		as++;
	}
	if(as > n - as)
	{
		cout << "Anton";
		return 0;
	}
	if(as < n - as)
	{
		cout <<  "Danik";
		return 0;
	}
	cout << "Friendship";
	return 0;
}
