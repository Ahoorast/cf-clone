#include <bits/stdc++.h>
using namespace std;
int main()
{
	string a;
	int n, ans = 0;
	cin >> n;
	for(int i = 0; i < n; i++)
	{
		cin >> a;
		if(a[0] == 'T')
		ans += 4;
		else if(a[0] == 'C')
		ans += 6;
		else if(a[0] == 'O')
		ans += 8;
		else if(a[0] == 'D')
		ans += 12;
		else if(a[0
		] == 'I')
		ans += 20;
	}
	cout << ans;
	return 0;
}
