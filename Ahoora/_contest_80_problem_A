#include <bits/stdc++.h>
using namespace std;
Â 
int main()
{
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);	
	int n, m;
	cin >> n >> m;
	for (int i = n + 1; i <= m + 1; i++) {
		int cnt = 0;
		for (int j = 1; j <= i; j++)
			if (i % j == 0) cnt++;
		if (cnt == 2) {
			cout << (i == m? "YES": "NO");
			return 0;
		}
	}
	cout << "NO";
	return 0;
}
