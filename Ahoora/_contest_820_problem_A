#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	int c, v0, v1, tmp = 0, res = 0, l, a, read;
	cin >> c >> v0 >> v1 >> a >> l;
	tmp = v0, res = 1, read += v0;
	while (read < c) {
		tmp += a;
		if (tmp > v1)
			tmp = v1;
		c += l;
		read += tmp;
		res++;
	}
	cout << res << endl;
	return 0;
}
