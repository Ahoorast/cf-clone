#include <bits/stdc++.h>
int arr[200];
using namespace std;
int main()
{
	string s;
	cin >> s;
	int sz = s.size(), ans = 0;
	for(int i = 0; i < sz; i++)
	if(s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u' || s[i] == '1' || s[i] == '3' || s[i] == '5' || s[i] == '7' || s[i] == '9')
	ans++;
	cout << ans;
	return 0;
}
