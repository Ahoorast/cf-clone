#include <bits/stdc++.h>
using namespace std;
int main()
{
	int a, b, ans;
	cin >> a >> b;
	ans = 6 - max(a, b) + 1;
	if(ans == 6) cout << "1/1";
	else if(ans == 5) cout << "5/6";
	else if(ans == 4) cout << "2/3";
	else if(ans == 3) cout << "1/2";
	else if(ans == 2) cout << "1/3";
	else if(ans == 1) cout << "1/6";
	else if(ans == 0) cout << "0/1";
	return 0;
}
