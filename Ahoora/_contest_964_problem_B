#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
 	ios_base:: sync_with_stdio(false), cin.tie(0), cout.tie(0);
	long long n, a, b, c, ti, t, res = 0, tmp = 0;
	cin >> n >> a >> b >> c >> ti;
	while (n--) {
		cin >> t;
		res += a + max(tmp, c - b) * (ti - t);
	}
	cout << res << endl;
	return 0;
}
