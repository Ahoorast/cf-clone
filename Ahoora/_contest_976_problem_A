#include <bits/stdc++.h>
using namespace std;
const int maxn = 1e5 + 100;
int p[maxn], q[maxn], arr[maxn];
int main()
{
	int n, cnt = 0, cnt2 = 0;
	string s;
	cin >> n >> s;
	for(int i = 0; i < n; i++) 
		if(s[i] == '0') cnt++;
		else cnt2++;	
	if(cnt2 == 0) cout << "0";
	else {
		cout << '1';
		for(int i = 0; i < cnt; i++) cout << '0';
	}	
	return 0;
}
