#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int n;
	cin >> n;
	n *= 2;
	vector<int> a(n);
	for (auto &x: a)
		cin >> x;
	int ans = 0;
	for (int i = 0; i < n; i++) {
		int p = -1;
		for (int j = i + 1; j < n; j++)
			if (a[j] == a[i])
				p = j;
		for (int j = p; j > i + 1; j--)
			swap(a[j], a[j - 1]), ans++;
	}
	cout << ans << '\n';
	return 0;
}
