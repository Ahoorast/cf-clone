#include <bits/stdc++.h>
using namespace std;
 
const int maxn = 1e5 + 100;
int a[2][maxn], n;
 
int main() {
 	ios_base:: sync_with_stdio(false), cin.tie(0), cout.tie(0);
	long long Q, l, r, d, res;
	cin >> Q;
	while (Q--) {
		cin >> l >> r >> d;
		if (d < l)
			cout << d << endl;
		else
			cout << d * (r / d) + d << endl;
	}
	return 0;
}
