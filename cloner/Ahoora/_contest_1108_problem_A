#include <bits/stdc++.h>
using namespace std;
const int maxn = 1e4 + 100;
int main()
{
	long long q, l1, r1, l2, r2;
	cin >> q;
	for(int i = 0; i < q; i++)
	{
		cin >> l1 >> r1 >> l2 >> r2;
		if(l1 != r2)
		cout << l1 << " " << r2 << endl;
		else if(r1 != l2)
		cout << r1 << ' ' << l2 << endl;
		else
		cout << l1 << " " << r2 - 1 << endl;
	}
	return 0;
}
