#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
	ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long x, y;
		cin >> x >> y;
		if (x == 1)
			cout << (y == 1? "YES": "NO") << endl;
		else if (x <= 3)
			cout << (y > 3? "NO": "YES") << endl;
		else
			cout << "YES" << endl;
	}
	return 0;
}
