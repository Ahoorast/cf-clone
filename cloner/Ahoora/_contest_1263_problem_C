#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
	ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q, n;
	cin >> Q;
	cout << endl << endl;
	while (Q--) {;
		cin >> n;
		set<int> st;
		for (int i = 1; 1ll * i * i <= n; i++)
			st.insert(n / i), st.insert(n / (i * i)), st.insert(i);
		st.insert(0);
		st.insert(n);
		st.insert(1);
		cout << st.size() << endl;
		while (st.size())
			cout << *st.begin() << ' ', st.erase(st.begin());
		cout << endl;
	}
	return 0;
}
