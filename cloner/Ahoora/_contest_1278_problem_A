#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		string p, h;
		cin >> p >> h;
		int pz = p.size(), hz = h.size();
		sort(p.begin(), p.end());
		bool res = false;
		for (int i = 0; i + pz <= hz; i++) {
			string t;
			for (int j = i; j < i + pz; j++) 
				t += h[j];
			sort(t.begin(), t.end());
			res |= t == p;
		}
		cout << (res? "YES": "NO") << endl;
	}
	return 0;
}
