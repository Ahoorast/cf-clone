#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
	ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	unsigned long long X, ans = 1LL << 59, A, B;
	cin >> X;
	vector<unsigned long long> p;
	for (unsigned long long i = 2; i * i <= X; i++) 
		if (X % i == 0) {
			unsigned long long x = 1;
			while (X % i == 0)
				X /= i, x *= i;
			p.push_back(x);
		}
	if (X > 1)
		p.push_back(X);
	int n = p.size();
	for (int i = 0; i < 1 << n; i++) {
		unsigned long long a = 1, b = 1;
		for (int j = 0; j < n; j++)
			if ((1 << j) & i)
				a *= p[j];
			else
				b *= p[j];
		if (ans > max(a, b)) {
			ans = max(a, b);
			A = a, B = b;
		}
	}
	cout << A << ' ' << B << endl;
	return 0;
}
