#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long s, ans = 0;
		cin >> s;
		while (s > 9) {
			long long t = s - s % 10;
			ans += t;
			s = s - t + t / 10;
		}
		cout << ans + s << endl;
	}
	return 0;
}
