#include<bits/stdc++.h>
using namespace std;
Â 
const int N = 1000 * 100 + 100;
int x, a[N], n;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		cin >> n >> x;
		int y = 0;
		set<int> s;
		for (int i = 1, a; i <= n; i++)
			cin >> a, y = max(y, 2 * a), s.insert(a);
		int ans = 0;
		while (x) {
			if (s.find(x) != s.end())
				x = 0, ans++;
			else {
				if (y >= x)
					ans += 2, x = 0;
				else {
					ans = x / (y / 2), x %= y / 2;
					if (x)
						ans--, x += y / 2;
				}
			}
		}
		cout << ans << endl;
	}
	return 0;
}
