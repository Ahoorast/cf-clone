#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int a, b;
		cin >> a >> b;
		if (a == b)
			cout << "0\n";
		else if (a < b)
			cout << ((b - a) & 1? 1: 2) << '\n';
		else
			cout << ((a - b) & 1? 2: 1) << '\n';
	}
	return 0;
}
