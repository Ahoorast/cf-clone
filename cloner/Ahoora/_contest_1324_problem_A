#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n, cnt[2] = {0, 0}, a;
		cin >> n;
		for (int i = 1; i <= n; i++)
			cin >> a, cnt[a % 2]++;
		if (cnt[1] > 0 && cnt[0] > 0)
			cout << "NO" << endl;
		else
			cout << "YES" << endl;
	}
	return 0;
}
