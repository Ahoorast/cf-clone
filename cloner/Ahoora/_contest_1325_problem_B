using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		vector<int> vec;
		cin >> n;
		for (int i = 1, a; i <= n; i++)
			cin >> a, vec.push_back(a);
		sort(vec.begin(), vec.end());
		vec.resize(distance(vec.begin(), unique(vec.begin(), vec.end())));
		cout << (int)(vec.size()) << '\n';
	}
	return 0;
}
