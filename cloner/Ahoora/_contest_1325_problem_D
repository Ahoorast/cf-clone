#include<bits/stdc++.h>
using namespace std;
Â 
int main() {
	long long s, x;
	cin >> x >> s;
	if ((x % 2 != s % 2) || (x > s))
		return cout << "-1\n", 0;
	if (s == 0)
		return cout << "0\n", 0;
	if (x == s)
		return cout << "1\n" << x << '\n', 0;
	long long ans[3] = {x, (s - x) / 2, (s - x) / 2};
	for (int i = 0; i < 60; i++)
		if ((!((1LL << i) & ans[0])) && ((ans[1] & (1LL << i))))
			ans[2] ^= (1LL << i), ans[0] ^= (1LL << i);
	if (ans[2])
		return cout << "3\n" << ans[0] << ' ' << ans[1] << ' ' << ans[2] << '\n', 0;
	cout << "2\n" << ans[0] << ' ' << ans[1] << '\n';
	return 0;
}
