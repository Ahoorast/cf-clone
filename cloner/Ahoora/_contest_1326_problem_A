#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		if (n == 1)
			cout << "-1\n";
		else {
			cout << 5;
			for (int i = 0; i < n - 1; i++)
				cout << 3;
			cout << '\n';
		}
	}
	return 0;
}
