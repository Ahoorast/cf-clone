#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int x, n, m;
		cin >> x >> n >> m;
		while (n--) 
			if (x / 2 + 10 <= x)
				(x /= 2) += 10;
		while (m--)
			x -= 10;
		cout << (x <= 0? "YES": "NO") << endl;
	}
	return 0;
}
