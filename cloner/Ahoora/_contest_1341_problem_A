#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n, a, b, c, d;
		cin >> n >> a >> b >> c >> d;
		long long l = n * (a - b), r = n * (a + b);
		if (l > c + d || r < c - d)
			cout << "No" << endl;
		else
			cout << "Yes" << endl;
	}
	return 0;
}
