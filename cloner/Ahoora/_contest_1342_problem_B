#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		string s;
		cin >> s;
		int n = s.size(), cnt[2] = {0, 0};
		for (int i = 0; i < n; i++)
			cnt[s[i] - '0']++;
		if (cnt[0] && cnt[1]) {
			for (int i = 0; i < 2 * n; i++)
				cout << (i & 1);
			cout << '\n';
		}
		else 
			cout << s << '\n';
	}
	return 0;
}
