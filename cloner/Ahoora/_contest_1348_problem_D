#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		vector<int> vec;
		vec.push_back(1);
		n--;
		int lst = 2;
		while (n - lst >= 0) {
			n -= lst;
			vec.push_back(lst);
			lst <<= 1;
		}
		int x = vec.size();
		if (n < vec[x - 1] && n > 0) {
			int d = vec[x - 1] - n;
			vec[x - 1] -= (d + 1) / 2, n += (d + 1) / 2;
		}
		if (n > 0)
			vec.push_back(n);
		cout << (int)(vec.size()) - 1 << '\n';
		for (int i = 1; i < vec.size(); i++)
			cout << vec[i] - vec[i - 1] << ' ';
		cout << '\n';
	}
	return 0;
}
