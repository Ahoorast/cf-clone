#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		if (n == 1)
			cout << 1 << endl;
		else if (n < 4)
			cout << -1 << endl;
		else if (n == 4)
			cout << 3 << ' ' << 1 << ' ' << 4 << ' ' << 2 << endl;
		else if (n == 5) 
			cout << 5 << ' ' << 3 << ' ' << 1 << ' ' << 4 << ' ' << 2 << endl;
		else {
			if (n & 1) {
				for (int i = n - 1; i > 4; i -= 2)
					cout << i << ' ';
				cout << 2 << ' ' << 4 << ' ';
				for (int i = 1; i <= n; i += 2)
					cout << i << ' ';
				cout << endl;
			}
			else {
				for (int i = n - 1; i > 5; i -= 2)
					cout << i << ' ';
				cout << 3 << ' ' << 1 << ' ' << 5 << ' ';
				for (int i = 2; i <= n; i += 2)
					cout << i << ' ';
				cout << endl;
			}
		}
	}
	return 0;
}
