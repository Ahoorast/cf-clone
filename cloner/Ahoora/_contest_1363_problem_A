#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n, x, a, cnt[2] = {0, 0};
		cin >> n >> x; 
		for (int i = 1; i <= n; i++)
			cin >> a, cnt[a & 1]++;
		bool res = false;
		for (int i = 1; i <= x; i += 2)
			if (cnt[1] >= i && cnt[0] >= x - i)
				res = true;
		cout << (res? "Yes": "No") << '\n';
	}
	return 0;
}
