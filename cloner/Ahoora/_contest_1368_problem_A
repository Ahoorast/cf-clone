#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long n, a, b;
		cin >> a >> b >> n;
		bool flag = true;
		int ans = 0;
		while (flag) {
			if (a > b)
				swap(a, b);
			a += b;
			ans++;
			if (a > n)
				flag = false;
		}
		cout << ans << '\n';
	}
	return 0;
}
