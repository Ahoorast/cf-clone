#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		bool fnd = false;
		for (int i = 2; i * i <= n; i++)
			if (n % i == 0 && !fnd) {
				cout << n / i << ' ' << n - (n / i) << '\n';
				fnd = true;
			}
		if (!fnd)
			cout << 1 << ' ' << n - 1 << '\n';
	}
	return 0;
}
