#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long a, b, c;
		cin >> a >> b >> c;
		cout << (a < c? 1: -1) << ' ';
		if (a * b > c)
			cout << b << '\n';
		else
			cout << -1 << '\n';
	}
	return 0;
}
