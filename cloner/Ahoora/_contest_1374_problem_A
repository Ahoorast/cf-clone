#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int x, y, n;
		cin >> x >> y >> n;
		int t = n / x;
		if (t * x + y <= n)
			cout << t * x + y << '\n';
		else
			cout << (t - 1) * x + y << '\n';
	}
	return 0;
}
