#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int a[3];
		for (int i = 0; i < 3; i++)
			cin >> a[i];
		sort(a, a + 3);
		if (a[1] != a[2])
			cout << "NO\n";
		else {
			cout << "YES\n";
			cout << a[2] << ' ' << a[0] << ' ' << a[0] << '\n';
		}
	}
	return 0;
}
