#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int r, g, b, w;
		cin >> r >> g >> b >> w;
		int cnt[2] = {0, 0};
		cnt[r % 2]++, cnt[g % 2]++, cnt[b % 2]++, cnt[w % 2]++;
		if (min(cnt[0], cnt[1]) > 1)
			cout << "No\n";
		else {
			if (cnt[1] <= 1)
				cout << "Yes\n";
			else 
				cout << (r > 0 && g > 0 && b > 0? "Yes": "No") << '\n';
		}
	}
	return 0;
}
