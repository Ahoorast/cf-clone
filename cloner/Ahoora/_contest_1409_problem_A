#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long a, b;
		cin >> a >> b;
		if (a > b)
			swap(b, a);
		long long t = (b - a) / 10;
		a += t * 10;
		cout << t + (a != b) << '\n';
	}
	return 0;
}
