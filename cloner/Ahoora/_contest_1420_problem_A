#include<bits/stdc++.h>
using namespace std;
Â 
const int N = 5 * 10000 + 100;
int n, a[N];
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		cin >> n;
		set<int> s;
		for (int i = 0; i < n; i++)
			cin >> a[i], s.insert(a[i]);
		reverse(a, a + n);
		cout << (is_sorted(a, a + n) && (int)(s.size()) == n? "NO": "YES") << '\n';
	}
	return 0;
}
