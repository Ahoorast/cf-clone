#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		int last = -1, ans = 0;
		for (int i = 0, a; i < n; i++) {
			cin >> a;
			if (a == 1) {
				if (last != -1)
					ans += i - last - 1;
				last = i;
			}
		}
		cout << ans << '\n';
	}
	return 0;
}
