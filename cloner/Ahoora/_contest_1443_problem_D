#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		vector<int> a(n);
		for (auto &x: a)
			cin >> x;
		for (int i = 1; i < n; i++)
			a[0] -= max(a[i - 1] - a[i], 0);
		cout << (a[0] < 0? "NO": "YES") << '\n';
	}
	return 0;
}
