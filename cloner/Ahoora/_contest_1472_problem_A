#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int h, w;
		cin >> h >> w;
		long long x = 1;
		while (h % 2 == 0) {
			h /= 2;
			x *= 2;
		}
		while (w % 2 == 0) {
			w /= 2;
			x *= 2;
		}
		long long n;
		cin >> n;
		cout << (x >= n? "YES\n": "NO\n");
	}
	return 0;
}
