#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		vector<int> a(n);
		map<int, int> cnt, num;
		for (auto &x: a)
			cin >> x, cnt[x]++;
		for (auto &x: a) 
			if (cnt.count(x)) {
				num[cnt[x]]++;
				cnt.erase(x);
			}
		int ans = n;
		int L = 0, R = 0, ted = 0;
		for (auto x: num) {
			ted += x.second;
			R += x.first * x.second;
		}
		for (auto x: num) {
			ted -= x.second;
			R -= x.first * x.second;
			ans = min(ans, L + R - ted * x.first);
			L += x.first * x.second;
		}
		cout << ans << '\n';
	}
	return 0;
}
