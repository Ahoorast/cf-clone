#include<bits/stdc++.h>
using namespace std;
 
 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		long long n, k;
		cin >> n >> k;
		for (int i = 0; i + 3 < k; i++)
			cout << 1 << ' ';
		n -= (k - 3);
		if (n % 4 == 0) 
			cout << n / 4 << ' ' << n / 4 << ' ' << n / 2 << '\n';
		 else if (n & 1)
			cout << 1 << ' ' << (n - 1) / 2 << ' ' << (n - 1) / 2 << '\n';
		else
			cout << 2 << ' ' << (n - 2) / 2 << ' ' << (n - 2) / 2 << '\n';
	}
	return 0;
}
