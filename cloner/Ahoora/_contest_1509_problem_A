#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	while (Q--) {
		int n;
		cin >> n;
		vector<int> vec[2];
		for (int i = 0, a; i < n; i++) {
			cin >> a;
			vec[a & 1].push_back(a);
		}
		for (int i = 0; i < 2; i++)
			for (auto x: vec[i])
				cout << x << ' ';
		cout << '\n';
	}
	return 0;
}
