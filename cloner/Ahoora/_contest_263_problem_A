#include <iostream>
using namespace std;
int abs(int n)
{
	int  ans = n;
	if(ans < 0)
	ans = -ans;
	return ans;
}
int main()
{
	int x, y, a;
	for(int i = 1; i <= 5; i++)
	{
		for(int j = 1; j <= 5; j++)
		{
			cin >> a;
			if(a == 1)
			{
				x = i;
				y = j;
			}
		}
	}
	cout << abs(x - 3) + abs(y - 3);
	return 0;
}
