#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int Q;
	cin >> Q;
	int mx = 1 << 30;
	int mn = -mx;
	map<string, string> ah;
	ah[">="] = "<";
	ah[">"] = "<=";
	ah["<="] = ">";
	ah["<"] = ">=";
	while (Q--) { 
		string s, ans;
		int x;
		cin >> s >> x >> ans;
		if (ans == "N")
			s = ah[s];
		if (s == ">=") {
			mn = max(mn, x);
		} else if (s == ">") {
			mn = max(mn, x + 1);
		} else if (s == "<=") {
			mx = min(mx, x);
		} else if (s == "<") {
			mx = min(mx, x - 1);
		}
	}
	if (mn > mx) 
		cout << "Impossible\n";
	else
		cout << mn << '\n';
	return 0;
}
