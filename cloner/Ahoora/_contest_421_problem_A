#include <bits/stdc++.h>
using namespace std;
const int maxn = 200;
int arr[maxn];
int main()
{
	int n, a, b, c;
	cin >> n >> a >> b;
	for(int i = 0; i < a; i++) {
		cin >> c;
		arr[c] = 1;
	}
	for(int i = 0; i < b; i++) {
		cin >> c;
		arr[c] = 2;
	}
	for(int i = 1; i <= n; i++) cout << arr[i] << ' ';
	return 0;
}
