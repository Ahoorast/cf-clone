#include <bits/stdc++.h>
using namespace std;
 
const int maxn = 200;
int a[maxn];
 
int main() {
 	ios_base:: sync_with_stdio(false), cin.tie(0), cout.tie(0);
	unsigned long long l, r;
	cin >> l >> r;
	if (r - l + 1 < 3 || (l % 2 == 1 && r - l + 1 < 4)) return cout << "-1", 0;
	if (l % 2 == 1)
		l++;
	for (unsigned long long i = l; i < l + 3; i++)
		cout << i << ' ';
	return 0;
}
