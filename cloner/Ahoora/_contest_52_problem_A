#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int n, a, cnt[3] = {0, 0, 0};
	cin >> n;
	for (int i = 0; i < n; i++) 
		cin >> a, cnt[a - 1]++;
	cout << cnt[0] + cnt[1] + cnt[2] - max({cnt[0], cnt[1], cnt[2]}) << endl;
	return 0;
}
