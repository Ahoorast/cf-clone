#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int n;
	cin >> n;
	vector<int> a(n);
	for (int i = 0; i < n; i++) {
		cin >> a[i];
		a[i] += i;
	}
	sort(a.begin(), a.end());
	for (int i = 1; i < n; i++)
		if (a[i] == a[i - 1])
			return cout << ":(\n", 0;
	for (int i = 0; i < n; i++)
		cout << a[i] - i << ' ';
	cout << '\n';
	return 0;
}
