#include <iostream>
using namespace std;
int main()
{
	int n, t;
	string a;
	cin >> n >> t;
	if(t == 10 && n < 2)
	{
		 cout << "-1";
		 return 0;
	}
	if(n == 0)
	{
		cout << "-1";
		return 0;
	}
	if(t == 10)
	{
		for(int i = 1; i < n; i++)
		cout << "1";
		cout << "0";
		return 0;
	}
	else
	{
		for(int i = 0; i < n; i++)
		cout << t;
	}
	return 0;
}
