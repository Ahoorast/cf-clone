#include <bits/stdc++.h>
using namespace std;
int main()
{
	string s;
	for(int i = 1; i < 10; i++) s += i + '0';
	for(int i = 1; i < 10; i++)
	for(int j = 0; j < 10; j++)
	{
		s += i + '0';
		s += j + '0';
	}
	for(int i = 1; i < 10; i++)
	for(int j = 0; j < 10; j++)
	for(int k = 0; k < 10; k++)
	{
		s += i + '0';
		s += j + '0';
		s += k + '0';
	}
	int n;
	cin >> n;
	cout << s[n - 1];
	return 0;
}
