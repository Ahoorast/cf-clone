#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	long long n, a, b, c, d, ans = 0;
	cin >> n >> a >> b >> c >> d;
	for (int i = 1; i <= n; i++) {
		int x = - a - b - i;
		int y = - b - d - i;
		int z = - d - c - i;
		int k = -c - a - i;
		long long mx = min({x, y, z, k});
		long long mn = max({x, y, z, k});
		mx *= -1, mn *= -1;
		ans += max(0LL, n + mn - mx);
	}
	cout << ans << '\n';
	return 0;
}
