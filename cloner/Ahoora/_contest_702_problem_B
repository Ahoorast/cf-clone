#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int n;
	cin >> n;
	unordered_map<long long, int> cnt;
	long long ans = 0;
	for (int i = 0, a; i < n; i++) {
		cin >> a;
		for (int i = 0; i < 60; i++) 
			ans += cnt[(1LL << i) - a];
		cnt[a]++;
	}
	cout << ans << '\n';
	return 0;
}
