#include <bits/stdc++.h>
using namespace std;
 
const int N = 200;
int n, a[N], t;
 
int main() {
	string s;
	cin >> s;
	int sz = s.size();
	for (int i = 0; i < sz; i++) {
		if (t == 0 && s[i] == 'h')
			t++;
		if (t == 1 && s[i] == 'e')
			t++;
		if (t == 2 && s[i] == 'i')
			t++;
		if (t == 3 && s[i] == 'd')
			t++;
		if (t == 4 && s[i] == 'i')
			t++;
	}
	if (t== 5)
		return cout << "YES", 0;
	cout << "NO";
	return 0;
}
