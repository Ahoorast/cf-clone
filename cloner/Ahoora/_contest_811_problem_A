int main() {
	long long a, b;
	cin >> a >> b;
	for (int i = 1; true; i++) {
		(i % 2 == 0? b -= i: a -= i);
//		cout << a << ' ' << b << endl;
		if (b < 0)
			return cout << "Valera", 0;
		else if (a < 0)
			return cout << "Vladik", 0;
	}
	return 0;
}
