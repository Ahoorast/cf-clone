/*
 
 
*/
#include <bits/stdc++.h>
using namespace std;
#define		debug(x)	cout << #x << '=' << x << endl
#define		print(x, n)	for(int i = 0; i < n; i++) cout << x[i] << ' ';
#define		pb			push_back
#define		sep			' '
 
int main()
{
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	long long n, a, res = 0;
	cin >> n;
	for(int i = 0; i < n; i++) cin >> a, res += a % 2;
	cout << (res >= 1 ? "First" : "Second");
	return 0;
}
