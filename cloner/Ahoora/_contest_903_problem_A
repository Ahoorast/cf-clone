#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
 	ios_base:: sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int n, x;
	cin >> n;
	while (n--) {
		cin >> x;
		int res = 0;
		if (x % 3 == 0 || x % 7 == 0)
			res = 1;
		for (int i = x; i > 0; i -= 3)
			if (i % 7 == 0)
				res = 1;
		cout << (res == 1? "YES": "NO") << endl;
	}
	return 0;
}
