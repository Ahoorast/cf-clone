#include <bits/stdc++.h>
using namespace std;
Â 
int main() {
 	ios_base:: sync_with_stdio(false), cin.tie(0), cout.tie(0);
	long long a, b, x, y, z, arr[2][2], res = 0, tmp = 0;
	for (int i = 0; i < 2; i++)
		cin >> arr[0][i];
	cin >> x >> y >> z;
	arr[1][0] = x * 2 + y;
	arr[1][1] = y + 3 * z;
	for (int i = 0; i < 2; i++)
		res += max(arr[1][i] - arr[0][i], tmp);
	cout << res << endl;
	return 0;
}
