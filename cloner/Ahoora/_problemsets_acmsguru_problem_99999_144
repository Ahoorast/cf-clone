#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int X, Y;
	double Z, ans = 0;
	cin >> X >> Y >> Z;
	double d = (Y - X) * 60;
	ans = (d - Z) / d * (Z / d) + Z / d;
	cout << setprecision(7) << fixed;
	cout << ans << '\n';
	return 0;
}
