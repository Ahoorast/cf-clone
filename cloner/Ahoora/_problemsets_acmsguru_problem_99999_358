#include<bits/stdc++.h>
using namespace std;
Â 
int main() { 
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	vector<int> vec;
	for (int i = 0; i < 3; i++) {
		vector<int> v(3);
		for (auto &x: v)
			cin >> x;
		sort(v.begin(), v.end());
		vec.push_back(v[1]);
	}
	sort(vec.begin(), vec.end());
	cout << vec[1] << '\n';
	return 0;
}
